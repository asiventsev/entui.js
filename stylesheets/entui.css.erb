.icon-add { background-image: url(<%= asset_path "entui/add.png" %>); }
.icon-move { background-image: url(<%= asset_path "entui/move.png" %>); }
.icon-edit { background-image: url(<%= asset_path "entui/edit.png" %>); }
.icon-cancel { background-image: url(<%= asset_path "entui/cancel.png" %>); }
.icon-false { background-image: url(<%= asset_path "entui/false.png" %>); }
.icon-zoom_in { background-image: url(<%= asset_path "entui/zoom_in.png" %>); }
.icon-reload { background-image: url(<%= asset_path "entui/reload.png" %>); }
